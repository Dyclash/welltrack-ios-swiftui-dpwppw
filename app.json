
{
  "expo": {
    "name": "Balance Day",
    "slug": "Balance Day",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/78bdd65a-d8c9-4585-a998-9ed0ed6f850b.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0A0E27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bagrun.balanceday",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/78bdd65a-d8c9-4585-a998-9ed0ed6f850b.png",
        "backgroundColor": "#0A0E27"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bagrun.balanceday"
    },
    "web": {
      "favicon": "./assets/images/78bdd65a-d8c9-4585-a998-9ed0ed6f850b.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "balanceday",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "Balance Day"
}
